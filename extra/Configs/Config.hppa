config TARGET_ARCH
	default "hppa"

config ARCH_SUPPORTS_BIG_ENDIAN
	bool
	default y

config ARCH_SUPPORTS_LITTLE_ENDIAN
	bool
	default n

config FORCE_OPTIONS_FOR_ARCH
	bool
	default y
	select ARCH_HAS_MMU
	select HAS_NO_THREADS
	select ARCH_HAS_NO_LDSO
	select HAVE_NO_SSP

config ARCH_CFLAGS
	string

config LIBGCC_CFLAGS
	string
