config ARCH_SUPPORTS_BIG_ENDIAN
	bool
	default y

config TARGET_ARCH
	select ARCH_HAS_NO_MMU
	select HAVE_NO_SHARED
	default "e1"

config ARCH_E1 
	bool
	default y

config ARCH_CFLAGS
	string
	default "-mgnu-param"

config LIBGCC_CFLAGS
	string
